{
  "name": "jest-to-match-sample-code",
  "version": "0.0.3",
  "description": "Matcher to check if the sample code in the document matches the contents of the source file.",
  "scripts": {
    "lint:eslint": "eslint ./src ./test ./sample",
    "lint:prettier": "prettier --check ./src ./test ./sample",
    "lint": "run-p lint:*",
    "build": "tsc -b ./src/tsconfig.json",
    "pretest": "npm run build",
    "test": "jest",
    "preversion": "npm test"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.6",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
